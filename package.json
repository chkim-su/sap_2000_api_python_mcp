{
  "name": "@tntndi001/sap2000-python-api",
  "version": "0.0.1",
  "description": "Node CLI wrapper to run the SAP2000 MCP Python stdio server (no ports).",
  "bin": {
    "sap2000-python-api": "bin/sap2000-mcp.js",
    "sap2000-mcp": "bin/sap2000-mcp.js"
  },
  "mcp": {
    "servers": {
      "sap2000": {
        "command": "sap2000-python-api",
        "args": [],
        "env": {
          "WORK_DIR": "{dataDir}/sap2000-mcp"
        }
      }
    }
  },
  "files": [
    "bin"
  ],
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": ["sap2000", "mcp", "server", "python", "uvicorn"],
  "license": "UNLICENSED"
}

